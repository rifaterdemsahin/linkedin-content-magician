# LinkedIn Content Magician - Environment Variables Example
# Copy this file to .env.development or .env.production and update values

# Production n8n Webhook URL
N8N_WEBHOOK_URL=https://n8n.rifaterdemsahin.com/webhook/normalize-data

# n8n Environment Configuration
N8N_HOST=n8n.rifaterdemsahin.com
N8N_PORT=443
N8N_PROTOCOL=https
N8N_WEBHOOK_ID=05c91180-4e19-4ccd-8917-658a96008ad9

# Application Configuration
APP_ENV=production
APP_NAME=linkedin-content-magician
APP_VERSION=1.0.0

# Debug Settings
DEBUG_MODE=false
LOG_LEVEL=info

# Timeout Settings (in milliseconds)
WEBHOOK_TIMEOUT=30000
RETRY_ATTEMPTS=3
RETRY_DELAY=1000

# Vite Environment Variables (for React app)
VITE_N8N_WEBHOOK_URL=https://n8n.rifaterdemsahin.com/webhook/normalize-data
VITE_DEBUG_MODE=false
VITE_APP_VERSION=1.0.0